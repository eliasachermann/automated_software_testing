                SELECT n, COUNT(*) AS count_matches 
                FROM counter 
                LEFT JOIN t_lHLm ON counter.n = CASE WHEN typeof(c_v1D) IN ('integer','real') THEN c_v1D ELSE NULL END
                GROUP BY n
                ORDER BY n
                LIMIT 20;

